<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/enumber"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ENumber">

    <EditText
        android:id="@+id/editText2"
        android:layout_width="299dp"
        android:layout_height="64dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="111dp"
        android:ems="10"
        android:fontFamily="@font/roboto_thin"
        android:gravity="center_horizontal"
        android:inputType="phone"
        android:textColor="#CBE9D7"
        android:textSize="24sp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="111dp"
        android:layout_marginEnd="109dp"
        android:layout_marginBottom="42dp"
        android:background="@android:color/transparent"
        android:textAllCaps="false"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/num0"
        android:layout_width="260dp"
        android:layout_height="33dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="130dp"
        android:fontFamily="@font/roboto_thin"
        android:text="0812345678"
        android:textColor="#CBE9D7"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/num1"
        android:layout_width="260dp"
        android:layout_height="33dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="170dp"
        android:fontFamily="@font/roboto_thin"
        android:text="0812345678"
        android:textColor="#CBE9D7"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/num2"
        android:layout_width="260dp"
        android:layout_height="33dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="210dp"
        android:fontFamily="@font/roboto_thin"
        android:text="0812345678"
        android:textColor="#CBE9D7"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/num3"
        android:layout_width="260dp"
        android:layout_height="33dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="250dp"
        android:fontFamily="@font/roboto_thin"
        android:text="0812345678"
        android:textColor="#CBE9D7"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/num4"
        android:layout_width="260dp"
        android:layout_height="33dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="290dp"
        android:fontFamily="@font/roboto_thin"
        android:text="0812345678"
        android:textColor="#CBE9D7"
        android:textSize="24sp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="34dp"
        android:layout_height="37dp"
        android:layout_alignBottom="@+id/num0"
        android:layout_marginStart="13dp"
        android:layout_marginBottom="0dp"
        android:layout_toEndOf="@+id/num0"
        android:onClick="del1"
        android:background="@android:color/transparent" />

    <Button
        android:id="@+id/button3"
        android:layout_width="34dp"
        android:layout_height="37dp"
        android:layout_alignBottom="@+id/num0"
        android:layout_marginStart="13dp"
        android:layout_marginBottom="-43dp"
        android:layout_toEndOf="@+id/num0"
        android:onClick="del2"
        android:background="@android:color/transparent" />

    <Button
        android:id="@+id/button4"
        android:layout_width="34dp"
        android:layout_height="37dp"
        android:layout_alignBottom="@+id/num0"
        android:layout_marginStart="13dp"
        android:layout_marginBottom="-81dp"
        android:layout_toEndOf="@+id/num0"
        android:onClick="del3"
        android:background="@android:color/transparent" />

    <Button
        android:id="@+id/button5"
        android:layout_width="34dp"
        android:layout_height="37dp"
        android:layout_alignBottom="@+id/num0"
        android:layout_marginStart="13dp"
        android:layout_marginBottom="-121dp"
        android:layout_toEndOf="@+id/num0"
        android:onClick="del4"
        android:background="@android:color/transparent" />

    <Button
        android:id="@+id/button6"
        android:layout_width="34dp"
        android:layout_height="37dp"
        android:layout_alignBottom="@+id/num0"
        android:layout_marginStart="14dp"
        android:layout_marginBottom="-159dp"
        android:layout_toEndOf="@+id/num0"
        android:onClick="del5"
        android:background="@android:color/transparent" />

</RelativeLayout>
